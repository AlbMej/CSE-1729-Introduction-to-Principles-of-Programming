"Problem 1"
( define ( harmonic n)
( if (= n 1)
1.0
(+ (/ 1 n) ( harmonic (- n 1)))))
 
(define (harmonic-iter n)
  (define (iterator a result count)
    ( if (= count 1)
         result
         (iterator (+ (/ 1 count) a) a (- count 1))))
  (iterator 0 0 n))

(harmonic 3)
(+ (harmonic-iter 3) 0.0)